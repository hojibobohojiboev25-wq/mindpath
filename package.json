    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": ["mind-map", "personality", "ai", "telegram", "nextjs", "express"],
